$("#post_food_comment").html(" <%= @post_food.post_food_comments.count %>");
$("#post_food_comment_<%= @post_food.id %>").html("<%= j(render 'post_food_comments/comments', post_food: @post_food) %>");
$("#post_food_comment_comment").val("");
<!--入力された際のアベレージ評価点数の表示-->
$("#post_food_rate").html("<%= @post_food.rate_avg %>");

<!--データ上書きにより余計に表示されるため一度星を空ににする-->
$("#star-rate-<%= @post_food.id%>").html('');
<!--削除された際の星個数の表示-->
$("#star-rate-<%= @post_food.id%>").raty({
  size: 36,
  starOff:'<%= asset_path('star-off.png') %>',
  starOn :'<%= asset_path('star-on.png') %>',
  starHalf:'<%= asset_path('star-half.png') %>',
  half: true,
  readOnly: true,
  score: <%= @rate_avg %>
});
 <!--ユーザーが空の状態で星評価を送信出来るにする。--> 
$("#star").html('');
$('#star').raty({
  size: 36,
  starOff:'<%= asset_path('star-off.png') %>',
  starOn :'<%= asset_path('star-on.png') %>',
  starHalf:'<%= asset_path('star-half.png') %>'
});